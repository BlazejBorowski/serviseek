{
    "name": "blazejborowski/laravel-value-objects",
    "description": "Laravel Value Objects package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "roninway",
        "blazejborowski",
        "laravel",
        "value-objects"
    ],
    "minimum-stability": "stable",
